  <dependencies>
    <dependency>
      <groupId>com.fantabel.humblebooks</groupId>
      <artifactId>HumbleBooksService</artifactId>
      <version>0.1-SNAPSHOT</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>com.fantabel.humblebooks</groupId>
      <artifactId>HumbleBooksPresentation</artifactId>
      <version>0.1-SNAPSHOT</version>
      <type>jar</type>
    </dependency>
